# Переменные окружения для Docker контейнеров SPIMEX парсера

# База данных PostgreSQL
DB_HOST=postgres
DB_PORT=5432
DB_USER=spimex_user
DB_PASS=spimex_password

# Названия баз данных
SYNC_DB_NAME=spimex_sync
ASYNC_DB_NAME=spimex_async

# API настройки
API_DEBUG=false
API_LOG_LEVEL=INFO

# Настройки Docker Compose
COMPOSE_PROJECT_NAME=spimex
COMPOSE_FILE=docker-compose.yml
